{
  "AppSettings": {
    "Secret": "#{Secret}#",
    "MessageToSign": "#{MessageToSign}#",
    "RefreshTokenTTL": 2
  },
  "ConnectionStrings": {
    "WebApiDatabase": "#{WebApiDatabase}#"
  },
  "BlobConnectionString": "#{BlobConnectionString}#",
  "BlobContainerName": "#{BlobContainerName}#",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}