{
    "AllowedHosts": "*",
    "ApplicationInsights": {
        "InstrumentationKey": "#{InstrumentationKey}#"
    },
    "AppSettings": {
        "Secret": "#{Secret}#",
        "RefreshTokenTTL": 2
    },
    "ConnectionStrings": {
        "WebApiDatabase": "#{WebApiDatabase}#",
        "AzureBlobStorage": "#{AzureBlobStorage}#"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "PushNotificationSettings": {
        "Apple": {
            "P8Key": "{P8Key}",
            "PrivateKey": "{PrivateKey}",
            "TeamId": "{TeamId}",
            "APPIdentifier": "com.strikingfinancial.Tribes",
            "Server": "{ApplePushServer}"
        }
    }
}