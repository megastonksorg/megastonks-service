{
  "AppSettings": {
    "Secret": "#{Secret}#",
    "MessageToSign": "#{MessageToSign}#",
    "RefreshTokenTTL": 2
  },
  "ConnectionStrings": {
    "WebApiDatabase": "#{WebApiDatabase}#",
    "AzureBlobStorage": "#{AzureBlobStorage}#"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "#{InstrumentationKey}#"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}